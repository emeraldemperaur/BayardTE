<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.LaunchActivity">

    <ImageView
        android:visibility="invisible"
        android:scaleType="fitXY"
        android:id="@+id/dev_intro_logo"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/me_code_blklogo_b"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="@string/meka_egwim" />

    <ImageView
        android:visibility="invisible"
        android:scaleType="fitXY"
        android:id="@+id/dev_intro_logo_spinner"
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:src="@drawable/me_code_blklogo_a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="@string/meka_egwim" />

</androidx.constraintlayout.widget.ConstraintLayout>